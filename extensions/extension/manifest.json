{
  "manifest_version": 2,
  "name": "Practice Extension",
  "version": "1",
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"]

  }
],
"permissions": ["tabs"],
"browser_action": {
  "default_icon": "mask.png",
  "default_popup": "sketch/index.html",
  "default_title": "Generate"
}
}
